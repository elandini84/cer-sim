<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>cer_ros2_control</name>
  <version>0.3.0</version>
  <description>
     An automatically generated package with all the configuration and launch files for using the SIM_CER_ROBOT with the MoveIt Motion Planning Framework
  </description>
  <maintainer email="ettore.landini@iit.it">Ettore Landini</maintainer>

  <license>BSD</license>

  <url type="website">http://moveit.ros.org/</url>
  <url type="bugtracker">https://github.com/ros-planning/moveit2/issues</url>
  <url type="repository">https://github.com/ros-planning/moveit2</url>

  <author email="ettore.landini@iit.it">Ettore Landini</author>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <exec_depend>tf2_ros</exec_depend>
  <exec_depend>xacro</exec_depend>
  <exec_depend>cer_rviz2</exec_depend>
  <exec_depend>controller_manager</exec_depend>
  <exec_depend>robot_state_publisher</exec_depend>
  <exec_depend>rviz2</exec_depend>
  <exec_depend>rviz_common</exec_depend>
  <exec_depend>rviz_default_plugins</exec_depend>
  <exec_depend>tf2_ros</exec_depend>

  <export>
      <build_type>ament_cmake</build_type>
  </export>
</package>
